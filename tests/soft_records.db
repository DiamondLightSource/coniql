record(longout, "$(P)longout") {
  field(HOPR, "100")
  field(HIHI, "98")
  field(HIGH, "96")
  field(DRVH, "90")
  field(DRVL, "10")
  field(LOW, "5")
  field(LOLO, "2")
  field(LOPR, "0")
  field(VAL, "$(VAL=42)")
  field(PINI, "YES")
}

record(stringin, "$(P)si") {
  field(VAL, "me")
}

record(waveform, "$(P)waveform") {
  field(NELM, "5")
  field(FTVL, "SHORT")
}

record(calc, "$(P)calc") {
  field(INPA, "$(P)longout CP")
  field(CALC, "A")
}

record(calc, "$(P)ticking") {
  field(INPA, "$(P)ticking")
  field(CALC, "A+1")
  field(SCAN, ".5 second")
}


