record(longout, "$(P)longout") {
  field(HOPR, "100")
  field(HIHI, "98")
  field(HIGH, "96")
  field(DRVH, "90")
  field(DRVL, "10")
  field(LOW, "5")
  field(LOLO, "2")
  field(LOPR, "0")
  field(HHSV, "MAJOR")
  field(HSV, "MINOR")
  field(LSV, "MINOR")
  field(LLSV, "MAJOR")
  field(VAL, "$(VAL=42)")
  field(PINI, "YES")
}

record(stringin, "$(P)si") {
  field(VAL, "me")
}

record(waveform, "$(P)waveform") {
  field(NELM, "5")
  field(FTVL, "DOUBLE")
  field(PREC, "1")
}

record(calc, "$(P)ticking") {
  field(INPA, "$(P)ticking")
  field(CALC, "A+1")
  field(SCAN, ".5 second")
  field(PREC, "5")
  field(EGU, "mm")
}

record(mbbi, "$(P)enum") {
    field(DESC, "Select the units of something")
    field(ZRST, "m")
    field(ZRVL, "0")
    field(ONST, "mm")
    field(ONVL, "1")
    field(TWST, "um")
    field(TWVL, "2")
    field(THST, "nm")
    field(THVL, "4")
    field(VAL, "3")
    field(PINI, "YES")
}

record(ao, "$(P)nan") {
  field(VAL, "NaN")
}
